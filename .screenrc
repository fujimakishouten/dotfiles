autodetach      on
defbce          on
defencoding     utf8
defhstatus      "Screen[^En]: ^Et"
defkanji        utf8
deflogin        off
defscrollback   200000
escape          ^b^b
hardstatus      alwayslastline "%{= gk}[%n] %w %= %H %02c %y/%m/%d"
startup_message off
term            xterm-256color
termcapinfo     xterm* 'Co#256:pa#32767:AB=\E[48;5;%dm:AF=\E[38;5;%dm:'
vbell           off

bind w windowlist -b
bind ^p paste
